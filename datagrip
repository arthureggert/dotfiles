#!/bin/bash
#Generated by JetBrains Toolbox 1.22.10970 at 2022-02-23T21:23:30.495442

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/aheggert/Library/Application Support/JetBrains/Toolbox/apps/datagrip/ch-0/213.6777.22/DataGrip.app/Contents/MacOS/datagrip" $wait --args "${ideargs[@]}"